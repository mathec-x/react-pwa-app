      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "_id": "react-pwa-app@0.1.3",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  },
  "dependencies": {
    "react": "^17.0.2"
  }
